{
  "openapi": "3.0.0",
  "info": {
    "title": "My API",
    "description": "API documentation",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:3001"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "operationId": "AppController_getHello",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/jobs": {
      "get": {
        "operationId": "AppController_getJobs",
        "responses": {
          "200": {
            "description": "List jobs",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/JobDto"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "JobDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "company": {
            "type": "string"
          }
        },
        "required": [
          "title",
          "company"
        ]
      }
    }
  }
}
